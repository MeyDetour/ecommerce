{% extends './admin/base.html.twig' %}
{% block title %}Nouvelle Catégorie{% endblock %}

{% block body %}
    <h1>Nouvelle categorie</h1>
    <div class="example-wrapper">
        {{ form_start(formCat) }}
        {{ form_row(formCat.name) }}
        <button type="submit" class="btn btn-primary">{{ cat is defined ? 'Modifier' : 'Créer' }}</button>
        {{ form_end(formCat) }}
    </div>
{% endblock %}

